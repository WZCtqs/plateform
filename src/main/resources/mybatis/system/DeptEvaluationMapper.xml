    <select id="selectDeptEvaluationList" parameterType="DeptEvaluation" resultMap="DeptEvaluationResult">
        <include refid="selectDeptEvaluationVo"/>
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        order by evaluation_time desc
    </select>

    <!--部门统计导出-->
    <select id="selectForExportSimple" parameterType="DeptEvaluation" resultType="DeptEvaluationExportSimple">

        SELECT '操作一' AS evaluationDept,avg( CZ1_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '集输部' AS evaluationDept,avg( JS_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '箱管一' AS evaluationDept,avg( XG1_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '箱管二' AS evaluationDept,avg( XG2_total ) AS score FROM dept_evaluation
