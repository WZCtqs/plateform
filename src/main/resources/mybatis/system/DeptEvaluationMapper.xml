            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '多式联运' AS evaluationDept,avg( DSLY_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '公路部' AS evaluationDept,avg( ROAD_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '报关一' AS evaluationDept,avg( BG1_total ) AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '报关二' AS evaluationDept,avg( BG2_total )  AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
        UNION ALL
        SELECT '跟单' AS evaluationDept,avg( GD_total )  AS score FROM dept_evaluation
        <where>
            <if test="startTime != null "> and evaluation_time &gt;= #{startTime}</if>
            <if test="endTime != null "> and evaluation_time &lt;= #{endTime}</if>
        </where>
    </select>

    <select id="selectDeptEvaluationById" parameterType="Long" resultMap="DeptEvaluationResult">
